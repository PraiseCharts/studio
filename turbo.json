{
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "@praisecharts/chordchartjs#build": {
      "outputs": [
        "lib/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "dependsOn": ["@praisecharts/chordchartjs#build"],
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
